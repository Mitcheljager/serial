    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.6.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.19.1",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.6.0",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/list": "^1.6.1",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/quote": "^2.4.0",
    "@jaames/iro": "^5.5.1",
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "5.2.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "sortablejs": "^1.13.0",
    "svelte": "^3.35.0",
    "svelte-inline-svg": "^1.0.8",
    "svelte-loader": "^3.0.0",
    "svelte-outside-click": "^1.1.1",
    "svelte-preprocess": "^4.7.0",
    "svelte-preprocess-sass": "^1.0.0",
    "svelte-spa-router": "^3.1.0",
    "turbolinks": "^5.2.0",
    "uuid": "^8.3.2",
    "webpacker-svelte": "^1.0.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^27.0.5",
    "jest": "^27.0.5",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "svelte-jester": "^1.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "jest": {
    "roots": [
      "spec/javascript"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": ["svelte-jester", { "preprocess": "./config/webpack/loaders/svelte.js" }]
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ],
    "moduleNameMapper": {
      "^@stores(.*)$": "<rootDir>/app/javascript/stores$1",
      "^@shared(.*)$": "<rootDir>/app/javascript/shared$1",
      "^@components(.*)$": "<rootDir>/app/javascript/components$1"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
